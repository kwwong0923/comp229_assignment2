<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ../partials/header.ejs %>
</head>
<body>
    <%- include ../partials/main_nav.ejs %>
    <main class="container">
        <div class="jumbotron">
            <h1>Message List</h1>
            <table>
                <tr>
                    <th>Name</th>
                    <th>Age</th>
                    <th>Phone</th>
                    <th>Emaill</th>
                    <th>Message</th>
                    <th></th>
                </tr>

                <% data.forEach((contact) => { %>
                    <tr>
                        <td><%= contact.name %></td>
                        <td><%= contact.age %></td>
                        <td><%= contact.phone %></td>
                        <td><%= contact.email %></td>
                        <td><%= contact.message %></td>
                        <td>
                            <a href="/contact/edit/<%= contact._id%>">Edit</a>
                        </td>
                        <td>
                            <a href="/contact/delete/<%= contact._id%>">Delete</a>
                        </td>
                    </tr>                
                <% }); %> 
            </table>
        </div>
    </main>
    <%- include ../partials/bottom_nav.ejs %>
    <%- include ../partials/footer.ejs %>
</body>
</html>